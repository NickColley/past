{
  "name": "past",
  "version": "0.0.1",
  "type": "module",
  "description": "Next.js inspired opinionated framework focusing on the boring past",
  "main": "index.mjs",
  "bin": {
    "past": "index.mjs"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint *.js && prettier --check *.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickColley/past.git"
  },
  "homepage": "https://github.com/NickColley/past#readme",
  "dependencies": {
    "express": "^4.18.1"
  },
  "engines": {
    "node": "^16.14.2"
  },
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  }
}
